{
  "timezone": "Etc/UTC",
  "extends": [
    "config:base",
    "schedule:daily"
  ],
  "dependencyDashboardLabels": ["dependencies"],
  "dependencyDashboardAutoclose": "true",
  "labels": ["dependencies"],
  "packageRules": [
    {
      "labels": ["helm"],
      "groupName": "helm charts",
      "matchManagers": ["helmv3", "helm-values"]
    },
    {
      "labels": ["cli"],
      "groupName": "golang",
      "matchManagers": ["gomod"]
    }
  ]
}
